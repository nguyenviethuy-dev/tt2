{
    "cli": {
      "version": ">= 15.0.0",
      "appVersionSource": "local"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "node": "20.11.0",
        "env": {
          "APP_ENV": "development"
        }
      },
      "preview": {
        "distribution": "internal",
        "android": {
          "buildType": "apk"
        },
        "node": "20.11.0",
        "env": {
          "APP_ENV": "preview"
        }
      },
      "production": {
        "node": "20.11.0",
        "env": {
          "APP_ENV": "production"
        }
      }
    }
  }