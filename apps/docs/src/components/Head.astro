---
import type { Props } from '@astrojs/starlight/props'
import Default from '@astrojs/starlight/components/Head.astro'
---

<!-- Render the default <Head/> component. -->
<Default {...Astro.props}><slot /></Default>

<!-- Render the <meta/> tags for the Open Graph images. -->
<meta property="og:image" content={`${Astro.url.origin}/rnr-banner.png`} />
<meta name="twitter:image" content={`${Astro.url.origin}/rnr-banner.png`} />