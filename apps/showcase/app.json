{
  "expo": {
    "name": "famvibe-app",
    "slug": "famvibe-app",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "example",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/d2cfcfcf-0dbe-4ae3-9050-641faf48090f"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#18181B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.reactnativereusables.showcase"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#18181B"
      },
      "package": "famvibe.comm"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    
    "extra": {
      "router": {
        "origin": false
        
      },
      "eas": {
        "projectId": "71686e93-b158-478c-87e3-e208ecb666a6"
      }
    },
    "cli": {
      "appVersionSource": "remote"
    },
    "owner": "nguyenviethuy-dev"
  }
}
