{
  "name": "@rnr/cli",
  "version": "0.0.1-rc.1",
  "description": "Add Universal components and primitives to your React-Native project.",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": {
    "name": "Zach Nugent",
    "url": "https://twitter.com/mrzachnugent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrzachnugent/react-native-reusables.git",
    "directory": "packages/cli"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "expo",
    "react-native",
    "universal-components",
    "components",
    "tailwind",
    "nativewind",
    "radix-ui",
    "shadcn"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist && rimraf components",
    "start:dev": "node dist/index.js",
    "start": "node dist/index.js",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "release": "changeset version",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta",
    "pub:next": "pnpm build && pnpm publish --no-git-checks --access public --tag next",
    "pub:release": "pnpm build && pnpm publish --access public",
    "test": "vitest run"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.4",
    "@babel/core": "^7.22.1",
    "@babel/parser": "^7.22.6",
    "@babel/plugin-transform-typescript": "^7.22.5",
    "@rnr/accordion": "*",
    "@rnr/alert-dialog": "*",
    "@rnr/aspect-ratio": "*",
    "@rnr/avatar": "*",
    "@rnr/checkbox": "*",
    "@rnr/collapsible": "*",
    "@rnr/context-menu": "*",
    "@rnr/dialog": "*",
    "@rnr/dropdown-menu": "*",
    "@rnr/hooks": "*",
    "@rnr/hover-card": "*",
    "@rnr/label": "*",
    "@rnr/menubar": "*",
    "@rnr/navigation-menu": "*",
    "@rnr/popover": "*",
    "@rnr/portal": "*",
    "@rnr/progress": "*",
    "@rnr/radio-group": "*",
    "@rnr/reusables": "*",
    "@rnr/select": "*",
    "@rnr/separator": "*",
    "@rnr/slider": "*",
    "@rnr/slot": "*",
    "@rnr/switch": "*",
    "@rnr/table": "*",
    "@rnr/tabs": "*",
    "@rnr/toast": "*",
    "@rnr/toggle": "*",
    "@rnr/toggle-group": "*",
    "@rnr/toolbar": "*",
    "@rnr/tooltip": "*",
    "@rnr/types": "*",
    "@rnr/utils": "*",
    "chalk": "5.2.0",
    "commander": "^10.0.0",
    "cosmiconfig": "^8.1.3",
    "diff": "^5.1.0",
    "execa": "^7.0.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.1.0",
    "https-proxy-agent": "^6.2.0",
    "lodash.template": "^4.5.0",
    "node-fetch": "^3.3.0",
    "ora": "^6.1.2",
    "prompts": "^2.4.2",
    "recast": "^0.23.2",
    "ts-morph": "^18.0.0",
    "tsconfig-paths": "^4.2.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.1",
    "@types/diff": "^5.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash.template": "^4.5.1",
    "@types/prompts": "^2.4.2",
    "rimraf": "^4.1.3",
    "tsup": "^6.6.3",
    "type-fest": "^3.8.0",
    "typescript": "^4.9.3"
  }
}
